{
  "name": "@vuldin/trie",
  "version": "2.0.0",
  "description": "rank text by best match of included words and phrases",
  "files": [
    "dist"
  ],
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.js"
  },
  "keywords": [
    "trie",
    "phrase",
    "word",
    "match",
    "stem",
    "search",
    "typeahead",
    "autocomplete"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist coverage vuldin-trie-*.tgz",
    "size": "size-limit",
    "test": "jest && npm run size",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "size-limit": [
    {
      "name": "CJS",
      "path": "dist/cjs/index.min.js",
      "limit": "1.8 KB"
    },
    {
      "name": "ESM",
      "path": "dist/esm/index.min.mjs",
      "limit": "1.8 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@size-limit/preset-small-lib": "^5.0.5",
    "babel-jest": "^25.1.0",
    "common-words": "^0.1.3",
    "jest": "^25.1.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.30.1",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.2.0",
    "size-limit": "^5.0.5",
    "stemmer": "^1.0.4"
  },
  "author": {
    "name": "Joshua Purcell",
    "email": "hello@vuld.in"
  },
  "repository": "https://github.com/vuldin/trie",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).?(m)js"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    }
  },
  "volta": {
    "node": "16.10.0"
  }
}
